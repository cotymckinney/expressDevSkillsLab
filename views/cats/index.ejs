<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=<device-width>, initial-scale=1.0">
<title>Express Cats</title>
</head>
<body>
<h1>
    Cats
</h1>
<h1>Meet the cats</h1>
<ul>
  <% cats.forEach(function(cat, idx) { %>
    <li>
      <a href='/cats/<%= idx %>'><%= cat.breed %></a>
        - 
      <%= cat.sex %>
        - 
      <img src="<%= cat.img %>"> 
    </li>
  <% }); %>
</ul>
<a href="/">
    Home
</a> 
</body>
</html>