<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=<device-width>, initial-scale=1.0">
<title>Express Dev Skills</title>
</head>
<body>
    <nav>
        <ul>
            <li>
                <a href="/">Home</a>
            </li>
        </ul>
    </nav>
    <div class='mainContainer'>
        <h1>My Dev Skills</h1>
        <ul>
        <% skill.forEach(function(skill, idx) { %>
            <li>
            <a href='/skills/<%= idx %>'><%= skill.unit %></a>
                - 
            <%= skill.name %>
                - 
            <img src="<%= skill.img %>"> 
            </li>
        <% }); %>
        </ul>
    </div>
</body>
</html>